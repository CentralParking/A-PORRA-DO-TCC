@model IEnumerable<CentralParking.Models.locador>

@{
    ViewBag.Title = "ListarLocador";
}

<h2>ListarLocador</h2>

<p>
    @Html.ActionLink("Criar novo", "Criar")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ID_Dono)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nome_Dono)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.senha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tipo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ID_Dono)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nome_Dono)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.senha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo)
        </td>
        <td>
            @Html.ActionLink("Editar", "Editar", new { id=item.CPF }) |
            @Html.ActionLink("Detalhes", "Detalhes", new { id=item.CPF }) |
            @Html.ActionLink("Deletar", "Deletar", new { id=item.CPF })
        </td>
    </tr>
}

</table>
